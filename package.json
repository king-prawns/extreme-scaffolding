{
  "name": "webpack3-starter-kit",
  "version": "1.0.1",
  "description": "webpack 3 scaffolding",
  "main": "./src/index.js",
  "engines": {
    "node": ">= 6.0",
    "npm": ">= 5.0",
    "yarn": ">= 1.1"
  },
  "scripts": {
    "dev": "node config/dev.js",
    "build": "node config/prod.js",
    "test": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/king-prawns/extreme-scaffolding.git"
  },
  "author": "Giorgio Gamberoni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/king-prawns/extreme-scaffolding/issues"
  },
  "homepage": "https://github.com/king-prawns/extreme-scaffolding#readme",
  "babel": {
    "comments": false,
    "compact": true,
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "ava": {
    "files": [
      "./src/**/*test.js"
    ],
    "failFast": true,
    "failWithoutAssertions": false,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "autoprefixer": "7.2.3",
    "ava": "0.24.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chalk": "2.3.0",
    "copy-webpack-plugin": "4.3.1",
    "css-loader": "0.28.7",
    "eslint": "4.14.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "fs-extra": "5.0.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.7.2",
    "nyc": "11.4.1",
    "opn": "5.1.0",
    "ora": "1.3.0",
    "postcss-loader": "2.0.9",
    "resolve-url-loader": "2.2.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0",
    "stylelint-webpack-plugin": "0.9.0",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-merge": "4.1.1"
  },
  "dependencies": {}
}
